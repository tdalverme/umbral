# ===========================================
# UMBRAL - Render (Background Worker)
# ===========================================
# Cargar estas variables en Render (Environment)

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# LLM Provider
LLM_PROVIDER=gemini

# Gemini (requerido para embeddings)
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-2.0-flash

# Groq (si LLM_PROVIDER=groq)
GROQ_API_KEY=your-groq-api-key-here
GROQ_MODEL=llama-3.1-8b-instant

# Telegram Bot
TELEGRAM_BOT_TOKEN=your-telegram-bot-token-here
TELEGRAM_WEBHOOK_URL=https://your-service.onrender.com
TELEGRAM_WEBHOOK_PATH=telegram

# Matching
SIMILARITY_THRESHOLD=0.85

# Logging
LOG_LEVEL=INFO

# Feedback learning rate
FEEDBACK_LEARNING_RATE=0.1

# Scraper trigger (Render)
SCRAPE_TRIGGER_TOKEN=your-secret-token-here
